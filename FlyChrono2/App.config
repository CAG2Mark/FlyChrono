<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="FlyChrono2.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="FlyChrono2.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
    </startup>
    <userSettings>
        <FlyChrono2.Properties.Settings>
            <setting name="AutoSync" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="AdvancedSync" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="ConstantlyCheckTime" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="ShowEtaAltitude" serializeAs="String">
                <value>10000</value>
            </setting>
            <setting name="SnoozeMinutes" serializeAs="String">
                <value>10</value>
            </setting>
            <setting name="AlarmSound" serializeAs="String">
                <value>C:\CAG2 Software\FlyChrono\AlarmSound.mp3</value>
            </setting>
