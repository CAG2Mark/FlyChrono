            </setting>
            <setting name="DatabaseRevision" serializeAs="String">
                <value>0</value>
            </setting>
        </FlyChrono2.Properties.Settings>
    </userSettings>
    <applicationSettings>
        <FlyChrono2.Properties.Settings>
            <setting name="ApplicationVersion" serializeAs="String">
                <value>OpenSource-Beta-1</value>
            </setting>
        </FlyChrono2.Properties.Settings>
    </applicationSettings>
